{
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx":true
        }
    },
    "env":{
        "browse":true,
        "node": true,
        "es6" : true
    },
    "extends":[
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins":[
        "import",
        "react-hooks"
    ]
}